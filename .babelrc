{
  "presets": [
    "react-native"
  ],
  "plugins": [
    "react-require",
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "alias": {          
          "@gutenberg": "./gutenberg"
        },
        "extensions": [
          ".js",
          ".native.js",
          ".ios.js",
          ".android.js"
        ]
      }
    ],
    "transform-async-to-generator",
    "@babel/plugin-proposal-async-generator-functions",
    "react-native-classname-to-style",
    [
      "react-native-platform-specific-extensions",
      {
        "extensions": [
          "css",
          "scss",
          "sass"
        ]
      }
    ]
  ],
  "env": {
    "development": {
      "plugins": [
        "@babel/transform-react-jsx-source"
      ]
    }
  }
}
